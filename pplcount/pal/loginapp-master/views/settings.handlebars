<h2 class="page-header">Camera Settings</h2>
{{#if errors}}
      {{#each errors}}
        <div class="alert alert-danger">{{msg}}</div>
      {{/each}}
{{/if}}
<form method="post" action="/users/settings">
   <div class="form-group">
    <label>Setting 1</label>
    <input type="text" class="form-control" placeholder="Setting 1" name="setting1">
  </div>
  <div class="form-group">
    <label>Setting 2</label>
    <input type="text" class="form-control" placeholder="Setting 2" name="setting2">
  </div>
   <div class="form-group">
    <label>Setting 3</label>
    <input type="code" class="form-control" placeholder="Setting 3" name="setting3">
  </div>
  <div class="form-group">
    <label>Setting 4</label>
    <input type="text" class="form-control" placeholder="Setting 4" name="setting4">
  </div>
  <div class="form-group">
    <label>Setting 5</label>
    <input type="text" class="form-control" placeholder="Setting 5" name="setting5">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>
